{
  "name": "php-fn/test",
  "homepage": "https://github.com/php-fn/php-fn-test",
  "license": "MIT",
  "require": {
    "markrogoyski/math-php": "^0",
    "phpunit/phpunit": "^7.5",

    "php": ">=7.1"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master"
  },
  "autoload": {
    "psr-4": {"fn\\": ["src/fn"]},
    "files": ["src/autoload-files.php"]
  },
  "autoload-dev": {
    "psr-4": {"fn\\": ["tests/fn"]}
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "extra": {
    "branch-alias": {
      "dev-master": "2.x-dev"
    }
  },
  "scripts": {
    "test": "php -d error_reporting=22527 vendor/bin/phpunit tests/"
  }
}
